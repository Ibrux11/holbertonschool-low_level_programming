#include "main.h"

/**
 * _isupper - checks for uppercase character
 * @c: the character to be checked
 *
 * Return: 1 if c is uppercase, 0 otherwise
 */
int _isupper(int c)
{
    return (c >= 'A' && c <= 'Z');
}
#include "main.h"
#include <stdio.h>

/**
*  print_alphabet - Prints the alphabet lowercase followed
*                   by a new line
*  Return: Always 0
*/

int _isupper(int c);

{
	char d = 'A';

	while (c <= 'A')
	{
		_putchar(c);
		c++;
	}
	_putchar('\n');
}
